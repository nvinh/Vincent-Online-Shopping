@model List<Models.EF.Menu>
<div class="menu">
    <ul>
        @foreach (var item in Model)
        {
            <li><a href="@item.Link" title="@item.Text" target="@item.Target">@item.Text</a></li>
        }
        @*<li class="active"><a href="index.html">Home</a></li>*@
        <div class="clear"></div>
    </ul>
</div>
